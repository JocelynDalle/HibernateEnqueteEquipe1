<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>EnquetesHibernateG7Depart</display-name>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SurveyServlet</servlet-name>
    <servlet-class>fr.humanbooster.fx.enquetes.servlets.SurveyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SurveyServlet</servlet-name>
    <url-pattern>/enquete</url-pattern>
    <url-pattern>/survey</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SurveysServlet</servlet-name>
    <servlet-class>fr.humanbooster.fx.enquetes.servlets.SurveysServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SurveysServlet</servlet-name>
    <url-pattern>/index</url-pattern>
    <url-pattern>/enquetes</url-pattern>
    <url-pattern>/surveys</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuestionAddServlet</servlet-name>
    <servlet-class>fr.humanbooster.fx.enquetes.servlets.QuestionAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuestionAddServlet</servlet-name>
    <url-pattern>/question</url-pattern>
  </servlet-mapping>
</web-app>